{
	"name": "m3 to datalake",
	"properties": {
		"activities": [
			{
				"name": "Copy m3 tables",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "@SELECT * FROM [M3FDBPRD].[MVXJDTA].[OOLINE]\nWHERE  \ncast( dateadd(s,CAST(OBLMTS/1000 AS decimal (13,0)),'19700101 05:00:00:000') as date)\n='pipeline().parameters.LoadingDate'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "m3tab",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OOLINE",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"LoadingDate": {
				"type": "string"
			}
		},
		"annotations": []
	}
}